#include <iostream>

int main() 
{
  std::cout << "_____________________________________________________________\n";
  std::cout << "|                            *                              |\n";
  std::cout << "|      *                                           *        |\n";
  std::cout << "|             __________________                            |\n";
  std::cout << "|            |                  |       (Hello!)            |\n";
  std::cout << "|            |  ()   ____   ()  |         ^                 |\n";
  std::cout << "|    *       |      |    |      |        O/            *    |\n";      
  std::cout << "|            |      |   .|      |       <|                  |\n";
  std::cout << "|            |      |    |      |       /|                  |\n";
  std::cout << "|-----------------------------------------------------------|\n";
}
